        <tbody>
         

            <% let counter=1%>
            <%   if(tickets.length) {%>
            <%tickets.forEach(function(ticket){%>


                <tr>
                    <td><%=counter%></td>
                    <td><%=ticket.ticketTitle%></td>
                    <td><%= ticket.raisedDate.getDate()%>-<%=ticket.raisedDate.getMonth() +1%>-<%=ticket.raisedDate.getFullYear()%></td>
                    <td><%=ticket.cName%></td>
                    <td><%=ticket.ticketStatus%></td>
                    <td><a class="btn btn-sm btn-primary" href="/view-ticket-details/<%=ticket._id%>">View Details</a></td>
                </tr>
                <%counter++%>
            <%})%>
            <%    } %>
            </tr>
        </tfoot>
    </tbody>
    </table>
