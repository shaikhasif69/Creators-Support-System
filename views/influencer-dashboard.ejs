            <div class="container-fluid pt-3 px-3">
                <div class="bg-light text-center rounded p-4">
            <div class="row g-4">
                <div class="bg-light text-center rounded p-4">
                    <div class="d-flex align-items-center justify-content-between mb-4">
                        <h6 class="mb-0">Your Tickets</h6>
                        <a href="raisedenquery.html">Show All</a>
                    </div>
                    <div class="table-responsive">
                        <table class="table text-start align-middle table-bordered table-hover mb-0">
                            <thead>
                                <tr class="text-dark">
                                    <th scope="col">Sr.</th>
                                    <th scope="col">Ticket Title</th>
                                    <th scope="col">Date</th>
                                    <th scope="col">Campaign names</th>
                                    <th scope="col">Status</th>
                                    <th scope="col">Action</th>
                                </tr>
                            </thead>
                            <tbody>
                                <% let counter=1%>
                                <%   if(tickets.length) {%>
                                <%tickets.forEach(function(ticket){%>
                                    <tr>
                                        <td><%=counter%></td>
                                        <td><%=ticket.ticketTitle%></td>
                                        <td><%= ticket.raisedDate.getDate()%>-<%=ticket.raisedDate.getMonth() +1%>-<%=ticket.raisedDate.getFullYear()%></td>
                                        <td>Campaign name </td>
                                        <td><%=ticket.ticketStatus%></td>
                                        <td><a class="btn btn-sm btn-primary" href="/view-ticket-details/<%=ticket._id%>">View Details</a></td>
                                    </tr>
                                    <%counter++%>
                                <%})%>
                                <%    } %>
                            </tbody>
                        </table>
                    </div>
